{
   "background": {
      "persistent": false,
      "scripts": [
         "clojure/compiled-dev/main.js",
         "dist/background.js"
      ]
   },
   "browser_action": {
      "default_popup": "popup/index.html",
      "default_title": "Chick"
   },
   "content_scripts": [
      {
         "js": [
            "dist/content.js"
         ],
         "matches": [
            "https://www.google.com/*",
            "https://www.google.co.jp/*",
            "https://www.bing.com/*",
            "https://duckduckgo.com/*",
            "https://search.yahoo.co.jp/*"
         ]
      }
   ],
   "description": "Chrome extension for full text bookmarks and browsing history search.",
   "icons": {
      "1024": "img/icon_1024.png",
      "128": "img/icon_128.png",
      "14": "img/icon_14.png",
      "16": "img/icon_16.png",
      "24": "img/icon_24.png",
      "256": "img/icon_256.png",
      "32": "img/icon_32.png",
      "48": "img/icon_48.png",
      "512": "img/icon_512.png",
      "64": "img/icon_64.png"
   },
   "manifest_version": 2,
   "name": "Chick",
   "omnibox": {
      "keyword": "e"
   },
   "options_page": "option/index.html",
   "permissions": [
      "tabs",
      "storage",
      "unlimitedStorage",
      "bookmarks",
      "history",
      "identity",
      "alarms",
      "http://*/",
      "https://*/"
   ],
   "version": "0.0.5"
}
